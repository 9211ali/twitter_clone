      </div>
  <% end %>
  <%= link_to '#', class: 'd-flex align-items-center fs-4 mb-4' do %>
    <div class="px-4 py-2 rounded-pill">
    <span class="me-4 position-relative" style="bottom: 3px"><%= image_tag "more.png", width: '32' %></span>More
      </div>
  <% end %>
  <div class="d-flex align-items-center fs-4 mb-4">
    <%= button_tag "Tweet", class: 'btn btn-primary btn-lg rounded-pill', 'data-bs-toggle': 'modal', 'data-bs-target': '#exampleModal' %>
  </div>
  <%= link_to 'Log out', destroy_user_session_path, data: { "turbo-method": :delete }, class: 'btn btn-primary' %>

<!-- Modal -->
<div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="btn-close m-0" data-bs-dismiss="modal" aria-label="Close" id="close-modal-btn"></button>
      </div>
      <div class="modal-body">
        <%= render 'shared/tweet_form' %>
      </div>
    </div>
  </div>
</div>